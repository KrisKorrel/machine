#!/bin/bash
#SBATCH -N 1
#SBATCH -p gpu_short
#SBATCH -t 0-01:00:00
#SBATCH -J exp2f
#SBATCH -n 2
#SBATCH -o exp2f

echo start of job

module load python
module load mkl
module load cuda/8.0.44

. ~/virtual_env_machine/bin/activate

cd ~/machine

time srun --multi-prog -l job_config/exp2f.conf

deactivate

echo end of job
